    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="janeiro, fevereiro, marco, abril, totaldevendas" type="Real" array="False" size=""/>
            <output expression="&quot;Total de vendas realizada em janeiro&quot;" newline="True"/>
            <input variable="janeiro"/>
            <output expression="&quot;Total de vendas realizada em fevereiro&quot;" newline="True"/>
            <input variable="fevereiro"/>
            <output expression="&quot;Total de vendas realizada em mar&#231;o&quot;" newline="True"/>
            <input variable="marco"/>
            <output expression="&quot;Total de vendas realizada em abril&quot;" newline="True"/>
            <input variable="abril"/>
