            <output expression="&quot;Total de valor vendido n&#243;s 4 meses: &quot;" newline="False"/>
            <output expression="totaldevendas" newline="True"/>
            <if expression="totaldevendas &gt;= 5000">
                <then>
                    <output expression="&quot;Parab&#233;ns!!! voc&#234; receber&#225; um abono de +10%&quot;" newline="True"/>
                </then>
                <else>
                    <output expression="&quot;Parab&#233;ns!!! Voc&#234; receber&#225; um abono de +3%&quot;" newline="True"/>
                </else>
            </if>
